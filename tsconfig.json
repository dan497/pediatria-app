{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "target": "ESNext",
    "lib": ["ESNext", "DOM"],
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
